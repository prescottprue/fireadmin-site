service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    // Files look like: "user/<UID>/path/to/file.txt"
    match /serviceAccounts/{projectId}/{userId}/{allPaths=**} {
      allow read, write: if request.auth.uid == userId;
    }
  }
}
